# Variáveis de Ambiente - Respawn Diário
# Renomeie este arquivo para .env e configure suas chaves

# JWT Secret para autenticação
JWT_SECRET=respawn_diario_jwt_secret_key_123456

# Chaves VAPID para Push Notifications
# Gere suas chaves em: https://vapidkeys.com/
VAPID_PUBLIC_KEY=SEU_VAPID_PUBLIC_KEY_AQUI
VAPID_PRIVATE_KEY=SEU_VAPID_PRIVATE_KEY_AQUI

# Google Analytics
GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX

# OAuth Keys
GOOGLE_CLIENT_ID=seu_google_client_id
GOOGLE_CLIENT_SECRET=seu_google_client_secret

FACEBOOK_APP_ID=seu_facebook_app_id
FACEBOOK_APP_SECRET=seu_facebook_app_secret

DISCORD_CLIENT_ID=seu_discord_client_id
DISCORD_CLIENT_SECRET=seu_discord_client_secret

# Database
DATABASE_URL=./data/respawn.db

# CORS Origins (para produção)
ALLOWED_ORIGINS=http://localhost:8080,https://respawndiario.com
